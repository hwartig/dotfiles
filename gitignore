*.iml
*.ipr
*.iws
.idea/


## Ignore Eclipse project & metadata
*.pydevproject
.project
.metadata
# be aware that gems and binstubs might have files in bin
#bin/** # not sure about this one
tmp/**
tmp/**/*
*.tmp
*.bak
*.swp
*~.nib
local.properties
.classpath
.settings/
.loadpath

# External tool builders
.externalToolBuilders/

# Locally stored "Eclipse launch configurations"
*.launch

# CDT-specific
.cproject

# PDT-specific
.buildpath


